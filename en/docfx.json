{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "bin/**/*.dll"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Stride Docs",
      "_enableSearch": true,
      "_appLogoPath": "/media/stride-logo-light.svg"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "tutorials/toc.yml",
          "tutorials/**/*.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/media/**/*.png",
          "**/media/**/*.jpg",
          "**/media/**/*.gif",
          "**/media/**/*.mp4",
          "**/media/**/*.zip",
          "**/media/**/*.svg",
          "**/media/**/*.webp",
          "**/media/**/*.cs"
        ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}