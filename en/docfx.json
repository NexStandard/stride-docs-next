{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "sources/core/Stride.Core/Stride.Core.csproj"
          ],
          "src": "../../stride",
          "filter": "filterConfig.yml"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net6.0-windows7.0",
        "StrideBuildDoc": "true"
      },
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Stride Docs",
      "_enableSearch": true,
      "_appLogoPath": "media/stride-logo-red.svg",
      "_appFooter": "<p>Hello World</p>"
    },
    "sitemap": {
      "baseUrl": "https://doc.stride3d.net/",
      "priority": 0.1,
      "changefreq": "monthly"
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "tutorials/toc.yml",
          "tutorials/**/*.md",
          "ReleaseNotes/toc.yml",
          "ReleaseNotes/**/*.md",
          "manual/toc.yml",
          "manual/**/*.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          ".nojekyll",
          "CNAME",
          "favicon.ico",
          "favicon.png",
          "images/**",
          "**/media/**/*.png",
          "**/media/**/*.jpg",
          "**/media/**/*.gif",
          "**/media/**/*.mp4",
          "**/media/**/*.zip",
          "**/media/**/*.svg",
          "**/media/**/*.webp",
          "**/media/**/*.cs"
        ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "markdownEngineProperties": {
      "toc": {
        "levels": "1,2,3"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}